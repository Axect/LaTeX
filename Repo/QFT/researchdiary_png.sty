\newcommand{\UP}{^}
\newcommandtwoopt{\OD}[2][][t]{\dfrac{d #1}{d #2}}                 % Ordinary Differentiate (default = d/dt)
\newcommand{\PD}[2]{\dfrac{\partial #1}{\partial #2}}              % Partial Differentiate
\newcommand{\PPD}[2]{\dfrac{\partial^2 #1}{\partial #2 ^2}}        % Double Partial
\newcommand{\tOD}[2]{\tfrac{d #1}{d #2}}                           % Small Ordinary Differntiate
\newcommand{\tPD}[2]{\tfrac{\partial #1}{\partial #2}}             % Small Partial Differntiate
\newcommand{\IN}[2]{\left( #1 \Com #2 \right)}                     % Bracket with comma
\newcommand{\Norm}[1]{\left\| #1 \right\|^2}                       % Norm 
\newcommand{\At}[2]{\left.#1\right|_{#2}}                          % Differentiate at ..
\newcommand{\HOT}[1]{\mathcal{O}\left( #1 \right)}                 % Big O function
\renewcommand{\O}{\mathcal{O}}                                     % Only Big O
\newcommand{\R}{\mathcal{R}}                                       % Ray
\newcommand{\eps}{\epsilon}                                        % epsilon
\newcommand{\X}{\vec{X}}                                           % Vector X
\newcommand{\q}{\dot{q}}                                           % dot q
\newcommand{\hq}{\hat{q}}                                          % hat q
\newcommand{\hp}{\hat{p}}                                          % hat p
\newcommand{\hX}{\hat{X}}                                          % hat X
\newcommand{\hP}{\hat{P}}                                          % hat P
\newcommand{\hH}{\hat{H}}                                          % hat H
\newcommand{\hA}{\hat{A}}                                          % hat A
\newcommand{\hU}{\hat{U}}                                          % hat U
\newcommandtwoopt{\LT}[2][\mu][\nu]{\tensor{\Lambda}{^#1 _#2}}     % Lorentz Transform
\newcommandtwoopt{\LTw}[2][\mu][\nu]{\tensor{\omega}{^#1 _#2}}     % Lorentz Transform generator
\newcommand{\Ex}[2]{\tensor[^\exists]{#1}{#2}}                     % Exist symbol
\newcommand{\TE}[1]{e^{#1 i\hH t}}                                 % Time Evolution Operator
\renewcommand{\L}{\mathcal{L}}                                     % symbol of lagrangian
\renewcommand{\H}{\mathcal{H}}                                     % symbol of Hilbert space
\newcommand{\Lag}[1][q_k(t), \dot{q}_k(t),t]{\mathcal{L}( #1 )}    % Default Lagrangian
\newcommand{\Act}[1][\Lag{}]{\int_{t_i}^{t_f}dt \hs #1}            % Action integral (default = Lagrangian)
\newcommand{\BKS}[1]{\left( #1 \right)}                            % Big bracket ()
\newcommand{\hket}[1][\psi]{\ket{#1}}                              % only ket
\newcommand{\sket}[1][\psi]{\tensor{\ket{#1}}{_s}}                 % Only Ket which is schrodinger
\newcommandtwoopt{\PKS}[2][][H]{\left\{ #1 , #2 \right\}}          % Poisson Bracket
\newcommandtwoopt{\Commutator}[2][A][\hH]{\left[ #1 , #2 \right] } % Commutator
\newcommand{\ACommutator}[2]{\{ #1 , #2 \}}                        % Anti Commutator
\newcommand{\TA}[3]{\bra{#1}e^{-i #2}\ket{#3}}                     % Transitional Amplitude
\newcommand{\Ip}{\int{d^3p}\hs}                                    % Integrate p^3
\newcommand{\vp}{\vec{p}\hs}                                       % Vector p and spacebar
\newcommand{\vq}{\vec{q}\hs}                                       % Vector q and spacebar
\newcommand{\abs}[1]{\left|#1\right|}                              % Absolute Value
\newcommand{\avp}{\abs{\vp}}                                       % Absolute vector p
\newcommand{\Dal}{\dfrac{\partial^2}{\partial t^2}-\nabla^2}       % D'Alembertian vector ver.
\newcommand{\KG}{(\Box + m^2)}                                     % Klein Gordon operator
\renewcommand{\u}[1][p]{u_{#1}}                                    % u_p
\newcommand{\Kin}{i(\u^*\partial^0\u-\partial^0\u^*\u)}            % Klein Gordon Inner product
\newcommandtwoopt{\KIn}[2][p][p']{i\int{d^3x}\hs{u}_{#1}^* \overset{\leftrightarrow}{\partial_0}{u}_{#2}} % Full ver. Klein Gordon Inner product
\newcommand{\DT}{\delta^{(3)}}                                     % Dirac delta in 3D
\newcommand{\Ep}{E_{\vp}}                                          % E_p
\newcommand{\J}[1]{\mathcal{J}^{#1}}                                         % Angular Momentum
\newcommand{\Px}{
\begin{pmatrix}
 0 & 1 \\
 1 & 0
\end{pmatrix}}                                                     % Pauli Matrix 1
\newcommand{\Py}{
\begin{pmatrix}
 0 & -i \\
 i & 0
\end{pmatrix}}                                                     % Pauli Matrix 2
\newcommand{\Pz}{
\begin{pmatrix}
 1 & 0 \\
 0 & -1
\end{pmatrix}}                                                     % Pauli Matrix 3
\newcommand{\Id}{
\begin{pmatrix}
 1 & 0 \\
 0 & 1
\end{pmatrix}}                                                     % Identity Matrix
